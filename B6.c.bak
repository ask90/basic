#include <stdio.h>

int main(void)
{
	int number, lastDigit = 0, preDigit = 0, identical =  0;
	scanf("%d", &number);
	while (number)
	{
		lastDigit = number % 10;
		preDigit = number % 100 / 10; 
		number /= 10;
		if (preDigit == lastDigit)
		{
			identical++;
		}
	}
	printf(identical ? "YES\n" : "NO\n");
    return 0;
}


